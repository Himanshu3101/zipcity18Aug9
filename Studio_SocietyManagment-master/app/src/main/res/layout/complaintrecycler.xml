                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="asdfga"
                android:fontFamily="@font/opensans_light"
                android:layout_marginLeft="15dp"
                android:textColor="#3e3e3e"
                android:textSize="13dp"/>

            <android.support.v7.widget.AppCompatTextView
                android:id="@+id/com_description_full"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="asdfga"
                android:visibility="gone"
                android:fontFamily="@font/opensans_light"
                android:layout_marginLeft="15dp"
                android:textColor="#3e3e3e"
                android:textSize="13dp"/>

        </android.support.v7.widget.LinearLayoutCompat>

        <android.support.v7.widget.LinearLayoutCompat
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:paddingBottom="20dp"
            android:layout_marginTop="100dp"
            android:layout_marginLeft="12dp"
            android:layout_marginRight="12dp">

        <android.support.v7.widget.LinearLayoutCompat
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_weight=".1">

            <android.support.v7.widget.AppCompatImageView
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_marginTop="5dp"
                android:background="@drawable/status"/>

            <android.support.v7.widget.AppCompatTextView
                android:id="@+id/com_status"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Accepted"
                android:layout_marginTop="5dp"
                android:layout_marginLeft="13dp"
                android:textColor="#3e3e3e"
                android:fontFamily="@font/opensans_light"
                android:textSize="13dp"/>

            <android.support.v7.widget.AppCompatTextView
                android:id="@+id/com_UniqCode"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:visibility="gone"
                android:layout_marginTop="5dp"
                android:layout_marginLeft="13dp"
                android:textColor="#3e3e3e"
                android:fontFamily="@font/opensans_light"
                android:textSize="13dp"/>

            <android.support.v7.widget.AppCompatTextView
                android:id="@+id/com_ComplaintID"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:visibility="gone"
                android:layout_marginTop="5dp"
                android:layout_marginLeft="13dp"
                android:textColor="#3e3e3e"
                android:fontFamily="@font/opensans_light"
                android:textSize="13dp"/>

        </android.support.v7.widget.LinearLayoutCompat>

        <android.support.v7.widget.LinearLayoutCompat
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_weight=".1">

            <android.support.v7.widget.AppCompatImageView
                android:layout_width="13dp"
                android:layout_marginLeft="20dp"
                android:layout_height="13dp"
                android:layout_marginTop="8dp"
                android:background="@drawable/hand_icon"/>

            <android.support.v7.widget.AppCompatTextView
                android:id="@+id/com_accepted"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Fateh singh"
                android:layout_marginTop="5dp"
                android:layout_marginLeft="4dp"
                android:textColor="#3e3e3e"
                android:fontFamily="@font/opensans_light"
                android:textSize="13dp"/>

        </android.support.v7.widget.LinearLayoutCompat>

        <android.support.v7.widget.LinearLayoutCompat
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_weight=".1">

            <android.support.v7.widget.AppCompatImageView
                android:layout_width="13dp"
                android:layout_height="13dp"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="10dp"
                android:background="@drawable/man_icon"/>

            <android.support.v7.widget.AppCompatTextView
                android:id="@+id/com_type"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Carpenter"
                android:textColor="#3e3e3e"
                android:layout_marginTop="6dp"
                android:fontFamily="@font/opensans_light"
                android:textSize="13dp"/>
        </android.support.v7.widget.LinearLayoutCompat>

        </android.support.v7.widget.LinearLayoutCompat>





