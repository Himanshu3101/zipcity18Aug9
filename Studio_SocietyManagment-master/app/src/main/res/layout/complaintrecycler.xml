<?xml version="1.0" encoding="utf-8"?>
<android.support.v7.widget.LinearLayoutCompat xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="#fff">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="7dp"
        android:layout_marginRight="7dp">

        <android.support.v7.widget.AppCompatTextView
            android:id="@+id/com_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="asdfga"
            android:textSize="15dp"
            android:fontFamily="@font/opensans_bold"
            android:textStyle="bold"
            android:textColor="#096a75" />

        <android.support.v7.widget.LinearLayoutCompat
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="25dp"
            android:layout_marginLeft="2dp">

        <android.support.v7.widget.LinearLayoutCompat
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_weight=".1"
            android:layout_marginLeft="10dp">

            <android.support.v7.widget.AppCompatImageView
                android:layout_width="12dp"
                android:layout_height="12dp"
                android:layout_marginTop="5dp"
                android:background="@drawable/created_on"/>

            <android.support.v7.widget.AppCompatTextView
                android:id="@+id/com_cre_on"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Createdon"
                android:fontFamily="@font/opensans_light"
                android:layout_marginLeft="13dp"
                android:textColor="#3e3e3e"
                android:textSize="13dp" />

        </android.support.v7.widget.LinearLayoutCompat>

        <android.support.v7.widget.LinearLayoutCompat
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_weight=".1"
            android:gravity="start"
            android:layout_marginLeft="20dp">

            <android.support.v7.widget.AppCompatImageView
                android:layout_width="13dp"
                android:layout_height="13dp"
                android:layout_marginTop="5dp"
                android:background="@drawable/created_by"/>

            <android.support.v7.widget.AppCompatTextView
                android:id="@+id/com_cre_by"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="CreatedBy"
                android:fontFamily="@font/opensans_light"
                android:layout_marginLeft="8dp"
                android:textColor="#3e3e3e"
                android:textSize="13dp" />

        </android.support.v7.widget.LinearLayoutCompat>

        </android.support.v7.widget.LinearLayoutCompat>

        <android.support.v7.widget.LinearLayoutCompat
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="50dp"
            android:layout_marginLeft="2dp">

            <android.support.v7.widget.LinearLayoutCompat
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_weight=".1"
                android:layout_marginLeft="10dp">

                <android.support.v7.widget.AppCompatImageView
                    android:layout_width="12dp"
                    android:layout_height="12dp"
                    android:layout_marginTop="5dp"
                    android:background="@drawable/map_icon"/>

                <android.support.v7.widget.AppCompatTextView
                    android:id="@+id/com_unit"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="flatno"
                    android:fontFamily="@font/opensans_light"
                    android:layout_marginLeft="13dp"
                    android:textColor="#3e3e3e"
                    android:textSize="13dp" />

            </android.support.v7.widget.LinearLayoutCompat>

            <android.support.v7.widget.LinearLayoutCompat
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_weight=".1"
                android:gravity="start"
                android:layout_marginLeft="20dp">

                <android.support.v7.widget.AppCompatImageView
                    android:layout_width="13dp"
                    android:layout_height="13dp"
                    android:layout_marginTop="5dp"
                    android:background="@drawable/request_no"/>

                <android.support.v7.widget.AppCompatTextView
                    android:id="@+id/com_complaintno"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="complaintno"
                    android:fontFamily="@font/opensans_light"
                    android:layout_marginLeft="8dp"
                    android:textColor="#3e3e3e"
                    android:textSize="13dp" />

            </android.support.v7.widget.LinearLayoutCompat>

        </android.support.v7.widget.LinearLayoutCompat>

        <android.support.v7.widget.LinearLayoutCompat
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="75dp"
            android:layout_marginLeft="12dp">

            <android.support.v7.widget.AppCompatImageView
                android:layout_width="13dp"
                android:layout_height="13dp"
                android:layout_marginTop="5dp"
