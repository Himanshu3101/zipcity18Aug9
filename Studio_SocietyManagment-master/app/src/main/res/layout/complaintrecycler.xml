<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="#f7f7f7"
    android:layout_marginRight="10dp"
    android:layout_marginLeft="10dp"
   android:layout_marginBottom="10dp">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <android.support.v7.widget.AppCompatTextView
            android:id="@+id/com_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="asdfga"
            android:textSize="15dp"
            android:fontFamily="@font/opensans_bold"
            android:textStyle="bold"
            android:textColor="#096a75" />

        <LinearLayout
           android:layout_width="match_parent"
           android:layout_height="wrap_content"
           android:orientation="horizontal"
           android:layout_marginLeft="7dp"
           android:layout_marginRight="7dp"
           android:layout_marginTop="5dp">

           <LinearLayout
               android:layout_width="match_parent"
               android:layout_height="wrap_content"
               android:orientation="horizontal"
               android:layout_weight=".1">

               <android.support.v7.widget.AppCompatImageView
                   android:layout_width="12dp"
                   android:layout_height="12dp"
                   android:layout_marginTop="3dp"
                   android:background="@drawable/created_on"/>

               <android.support.v7.widget.AppCompatTextView
                   android:id="@+id/com_cre_on"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:text="Createdon"
                   android:fontFamily="@font/opensans_light"
                   android:layout_marginLeft="13dp"
                   android:textColor="#3e3e3e"
                   android:textSize="12dp" />
           </LinearLayout>
           <LinearLayout
               android:layout_width="match_parent"
               android:layout_height="wrap_content"
               android:orientation="horizontal"
               android:layout_weight=".1"
               android:layout_marginLeft="5dp">

               <android.support.v7.widget.AppCompatImageView
                   android:layout_width="13dp"
                   android:layout_height="13dp"
                   android:layout_marginTop="3dp"
                   android:background="@drawable/created_by"/>

               <android.support.v7.widget.AppCompatTextView
                   android:id="@+id/com_cre_by"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:text="CreatedBy"
                   android:fontFamily="@font/opensans_light"
                   android:layout_marginLeft="8dp"
                   android:textColor="#3e3e3e"
                   android:textSize="12dp" />
           </LinearLayout>

       </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginLeft="7dp"
            android:layout_marginRight="7dp"
            android:layout_marginTop="5dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_weight=".1">

                <android.support.v7.widget.AppCompatImageView
                    android:layout_width="12dp"
                    android:layout_height="12dp"
                    android:layout_marginTop="5dp"
                    android:background="@drawable/map_icon"/>

                <android.support.v7.widget.AppCompatTextView
                    android:id="@+id/com_unit"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="flatno"
                    android:fontFamily="@font/opensans_light"
                    android:layout_marginLeft="13dp"
                    android:textColor="#3e3e3e"
                    android:textSize="12dp" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_weight=".1"
                android:layout_marginLeft="5dp">

                <android.support.v7.widget.AppCompatImageView
                    android:layout_width="13dp"
                    android:layout_height="13dp"
                    android:layout_marginTop="5dp"
                    android:background="@drawable/request_no"/>

                <android.support.v7.widget.AppCompatTextView
                    android:id="@+id/com_complaintno"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="complaintno"
                    android:fontFamily="@font/opensans_light"
                    android:layout_marginLeft="8dp"
                    android:textColor="#3e3e3e"
                    android:textSize="12dp" />
            </LinearLayout>

        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginLeft="7dp"
            android:layout_marginRight="7dp"
            android:layout_marginTop="5dp">

               <android.support.v7.widget.AppCompatImageView
                    android:layout_width="13dp"
                    android:layout_height="13dp"
                    android:layout_marginTop="5dp"
                    android:background="@drawable/description_icon"/>

                <android.support.v7.widget.AppCompatTextView
                    android:id="@+id/com_description"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="asdfga"
                    android:fontFamily="@font/opensans_light"
                    android:layout_marginLeft="15dp"
                    android:textColor="#3e3e3e"
                    android:textSize="12dp"/>

                <android.support.v7.widget.AppCompatTextView
                    android:id="@+id/com_description_full"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="asdfga"
                    android:visibility="gone"
                    android:fontFamily="@font/opensans_light"
                    android:layout_marginLeft="15dp"
                    android:textColor="#3e3e3e"
                    android:textSize="12dp"/>
            </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginLeft="7dp"
            android:layout_marginRight="7dp"
            android:layout_marginTop="5dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_weight=".1">

                <android.support.v7.widget.AppCompatImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_marginTop="5dp"
                    android:background="@drawable/status"/>

                <android.support.v7.widget.AppCompatTextView
                    android:id="@+id/com_status"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Accepted"
                    android:layout_marginTop="5dp"
                    android:layout_marginLeft="5dp"
                    android:textColor="#3e3e3e"
                    android:fontFamily="@font/opensans_light"
                    android:textSize="12dp"/>

                <android.support.v7.widget.AppCompatTextView
                    android:id="@+id/com_UniqCode"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:visibility="gone"
                    android:layout_marginTop="5dp"
                    android:layout_marginLeft="13dp"
                    android:textColor="#3e3e3e"
                    android:fontFamily="@font/opensans_light"
                    android:textSize="13dp"/>

                <android.support.v7.widget.AppCompatTextView
                    android:id="@+id/com_ComplaintID"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:visibility="gone"
                    android:layout_marginTop="5dp"
                    android:layout_marginLeft="13dp"
                    android:textColor="#3e3e3e"
                    android:fontFamily="@font/opensans_light"
                    android:textSize="12dp"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_weight=".1">

                <android.support.v7.widget.AppCompatImageView
                    android:layout_width="13dp"
                    android:layout_marginLeft="10dp"
                    android:layout_height="13dp"
                    android:layout_marginTop="8dp"
                    android:background="@drawable/hand_icon"/>

                <android.support.v7.widget.AppCompatTextView
                    android:id="@+id/com_accepted"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Fateh singh"
                    android:layout_marginTop="5dp"
                    android:layout_marginLeft="5dp"
                    android:textColor="#3e3e3e"
                    android:fontFamily="@font/opensans_light"
                    android:textSize="12dp"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_weight=".1">

                <android.support.v7.widget.AppCompatImageView
                    android:layout_width="13dp"
                    android:layout_height="13dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="10dp"
                    android:background="@drawable/man_icon"/>

                <android.support.v7.widget.AppCompatTextView
                    android:id="@+id/com_type"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Carpenter"
                    android:textColor="#3e3e3e"
                    android:layout_marginTop="6dp"
                    android:layout_marginLeft="5dp"
                    android:fontFamily="@font/opensans_light"
                    android:textSize="12dp"/>
            </LinearLayout>

        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1.5dp"
            android:background="#d3d3d3"
            android:layout_marginTop="7dp"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"/>

    </LinearLayout>

    <android.support.v7.widget.AppCompatButton
        android:id="@+id/btnfrComplaintShow"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="08dp"
        android:background="@color/transparent"/>

</FrameLayout>
