<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="165dp"
    android:layout_marginBottom="10dp"
    android:background="#f7f7f7">

    <LinearLayout
        android:id="@+id/ll"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:layout_marginRight="10dp"
        android:layout_marginLeft="10dp"
        android:background="@drawable/popupbackground">

        <android.support.v7.widget.AppCompatTextView
            android:id="@+id/com_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="asdfga"
            android:textSize="15dp"
            android:fontFamily="@font/opensans_bold"
            android:textStyle="bold"
            android:textColor="#096a75"
            android:inputType="textCapSentences"/>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="#096a75"
            android:layout_marginTop="10dp"
            />

        <LinearLayout
           android:layout_width="match_parent"
           android:layout_height="wrap_content"
           android:orientation="horizontal"
           android:layout_marginLeft="7dp"
           android:layout_marginRight="7dp"
           android:layout_marginTop="5dp">

           <!--<LinearLayout-->
               <!--android:layout_width="match_parent"-->
               <!--android:layout_height="wrap_content"-->
               <!--android:orientation="horizontal"-->
               <!--android:layout_weight=".1">-->

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight=".1">

               <android.support.v7.widget.AppCompatImageView
                   android:layout_width="11dp"
                   android:layout_height="11dp"
                   android:layout_marginTop="3dp"
                   android:background="@drawable/created_on"/>

               <android.support.v7.widget.AppCompatTextView
                   android:id="@+id/com_cre_on"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:text="Createdon"
                   android:fontFamily="@font/opensans_light"
                   android:layout_marginLeft="15dp"
                   android:textColor="#3e3e3e"
                   android:textSize="12dp" />

            </RelativeLayout>
           <!--</LinearLayout>-->

           <!--<LinearLayout-->
               <!--android:layout_width="match_parent"-->
               <!--android:layout_height="wrap_content"-->
               <!--android:orientation="horizontal"-->
               <!--android:layout_weight=".1"-->
               <!--android:layout_marginLeft="5dp">-->

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight=".15">

               <android.support.v7.widget.AppCompatImageView
                   android:id="@+id/img"
                   android:layout_width="13dp"
                   android:layout_height="13dp"
                   android:layout_marginTop="3dp"

                   android:background="@drawable/created_by"/>

               <android.support.v7.widget.AppCompatTextView
                   android:id="@+id/com_cre_by"
                   android:layout_alignLeft="@+id/img"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:text="CreatedBy"
                   android:fontFamily="@font/opensans_light"
                   android:layout_marginLeft="15dp"
                   android:textAlignment="textEnd"
                   android:textColor="#3e3e3e"
                   android:textSize="12dp" />

            </RelativeLayout>
           <!--</LinearLayout>-->

       </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginLeft="7dp"
            android:layout_marginRight="7dp"
            android:layout_marginTop="5dp">

            <!--<LinearLayout-->
                <!--android:layout_width="match_parent"-->
                <!--android:layout_height="wrap_content"-->
                <!--android:orientation="horizontal"-->
                <!--android:layout_weight=".1">-->

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight=".1">

                <android.support.v7.widget.AppCompatImageView
                    android:layout_width="12dp"
                    android:layout_height="12dp"
                    android:layout_marginTop="5dp"
                    android:background="@drawable/map_icon"/>

                <android.support.v7.widget.AppCompatTextView
                    android:id="@+id/com_unit"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="flatno"
                    android:fontFamily="@font/opensans_light"
                    android:layout_marginLeft="15dp"
                    android:layout_marginTop="1dp"
                    android:textColor="#3e3e3e"
                    android:textSize="12dp" />

            </RelativeLayout>
            <!--</LinearLayout>-->
            <!--<LinearLayout-->
                <!--android:layout_width="match_parent"-->
                <!--android:layout_height="wrap_content"-->
                <!--android:orientation="horizontal"-->
                <!--android:layout_weight=".1"-->
                <!--android:layout_marginLeft="5dp">-->

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight=".15">

                <android.support.v7.widget.AppCompatImageView
                    android:layout_width="13dp"
                    android:layout_height="13dp"
                    android:layout_marginTop="5dp"
                    android:background="@drawable/request_no"/>

                <android.support.v7.widget.AppCompatTextView
                    android:id="@+id/com_complaintno"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Complaintno"
                    android:fontFamily="@font/opensans_light"
                    android:layout_marginLeft="15dp"
                    android:layout_marginTop="02dp"
                    android:textColor="#3e3e3e"
                    android:textSize="12dp" />
            <!--</LinearLayout>-->
            </RelativeLayout>





        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginLeft="7dp"
            android:layout_marginRight="7dp"
            android:layout_marginTop="5dp">

               <android.support.v7.widget.AppCompatImageView
                    android:layout_width="13dp"
                    android:layout_height="13dp"
                    android:layout_marginTop="5dp"
                    android:background="@drawable/description_icon"/>

                <android.support.v7.widget.AppCompatTextView
                    android:id="@+id/com_description"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="asdfga"
                    android:fontFamily="@font/opensans_light"
                    android:layout_marginLeft="03dp"
                    android:layout_marginTop="1dp"
                    android:textColor="#3e3e3e"
                    android:textSize="12dp"/>

                <android.support.v7.widget.AppCompatTextView
                    android:id="@+id/com_description_full"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="asdfga"
                    android:visibility="gone"
                    android:fontFamily="@font/opensans_light"
                    android:layout_marginLeft="15dp"
                    android:textColor="#3e3e3e"
                    android:textSize="12dp"/>
            </LinearLayout>



        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_gravity="bottom"
            android:layout_marginTop="10dp"
            android:background="@drawable/shape" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginLeft="11dp"
                android:layout_weight=".1">

                <android.support.v7.widget.AppCompatImageView
                    android:layout_width="18dp"
                    android:layout_height="18dp"
                    android:layout_marginTop="2dp"
                    android:background="@drawable/status"/>

                <android.support.v7.widget.AppCompatTextView
                    android:id="@+id/com_status"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Accepted"
                    android:layout_marginTop="3dp"
                    android:layout_marginLeft="5dp"
                    android:textColor="#3e3e3e"
                    android:fontFamily="@font/opensans_light"
                    android:textSize="12dp"/>

                <android.support.v7.widget.AppCompatTextView
                    android:id="@+id/com_UniqCode"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:visibility="gone"
                    android:layout_marginTop="5dp"
                    android:layout_marginLeft="13dp"
                    android:textColor="#3e3e3e"
                    android:fontFamily="@font/opensans_light"
                    android:textSize="13dp"/>

                <android.support.v7.widget.AppCompatTextView
                    android:id="@+id/com_ComplaintID"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:visibility="gone"
                    android:layout_marginTop="5dp"
                    android:layout_marginLeft="13dp"
                    android:textColor="#3e3e3e"
                    android:fontFamily="@font/opensans_light"
                    android:textSize="12dp"/>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginLeft="10dp"
                android:layout_weight=".1">

                <android.support.v7.widget.AppCompatImageView
                    android:layout_width="13dp"
                    android:layout_marginLeft="10dp"
                    android:layout_height="13dp"
                    android:layout_marginTop="5dp"
                    android:background="@drawable/hand_icon"/>

                <android.support.v7.widget.AppCompatTextView
                    android:id="@+id/com_accepted"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Fateh singh"
                    android:layout_marginTop="3dp"
                    android:layout_marginLeft="5dp"
                    android:textColor="#3e3e3e"
                    android:fontFamily="@font/opensans_light"
                    android:textSize="12dp"/>
            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_weight=".1">

                <android.support.v7.widget.AppCompatImageView
                    android:layout_width="13dp"
                    android:layout_height="13dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="5dp"
                    android:background="@drawable/man_icon"/>

                <android.support.v7.widget.AppCompatTextView
                    android:id="@+id/com_type"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Carpenter"
                    android:textColor="#3e3e3e"
                    android:layout_marginTop="3dp"
                    android:layout_marginLeft="5dp"
                    android:fontFamily="@font/opensans_light"
                    android:textSize="12dp"/>
            </LinearLayout>

        </LinearLayout>





    </LinearLayout>



    <android.support.v7.widget.AppCompatButton
        android:id="@+id/btnfrComplaintShow"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="08dp"
        android:background="@color/transparent"/>

</FrameLayout>
